<h1>Listing archives</h1>

<table>
  <tr>
    <th>Pickuparchiveid</th>
    <th>Padefaultrouteid</th>
    <th>Papickupdate</th>
    <th>Pacustomerid</th>
    <th>Padrivername</th>
    <th>Padrivermessage</th>
    <th>Paroutename</th>
    <th>Pacustomername</th>
    <th>Pacomment</th>
    <th>Padispatched</th>
    <th>Padispatchtime</th>
    <th>Padeleted</th>
    <th>Panotes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @archives.each do |archive| %>
  <tr>
    <td><%= archive.PickupArchiveID %></td>
    <td><%= archive.PADefaultRouteID %></td>
    <td><%= archive.PAPickupDate %></td>
    <td><%= archive.PACustomerID %></td>
    <td><%= archive.PADriverName %></td>
    <td><%= archive.PADriverMessage %></td>
    <td><%= archive.PARouteName %></td>
    <td><%= archive.PACustomerName %></td>
    <td><%= archive.PAComment %></td>
    <td><%= archive.PADispatched %></td>
    <td><%= archive.PADispatchTime %></td>
    <td><%= archive.PADeleted %></td>
    <td><%= archive.PANotes %></td>
    <td><%= link_to 'Show', archive %></td>
    <td><%= link_to 'Edit', edit_archive_path(archive) %></td>
    <td><%= link_to 'Destroy', archive, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

