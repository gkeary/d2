rails generate scaffold Customer CustomerTableID:integer CustomerID:string CustomerName:string Address1:string Address2:string City:string State:string Phone:string Contact:string ClosingTime:string Comment:string DefaultRouteID:string DUser:string Terminal:string InfoString:string 

rails g scaffold Pickup PickupTableID:integer DefaultRouteID:string PickupDate:datetime CustomerID:string DriverName:string RouteName:string CustomerName:string Comment:string Monday:boolean Tuesday:boolean Wednesday:boolean Thursday:boolean Friday:boolean Dispatched:boolean DispatchTime:datetime Notes:string 


rails g scaffold Archive PickupArchiveID:integer PADefaultRouteID:string PAPickupDate:datetime PACustomerID:string PADriverName:string PADriverMessage:string PARouteName:string PACustomerName:string PAComment:string PADispatched:boolean PADispatchTime:datetime PADeleted:boolean PANotes:string 

